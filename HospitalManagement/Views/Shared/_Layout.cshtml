                    const $el = $('#data-frame-times');
                    $el.html();
                    const data = [];
                    let rawItem = "";
                    res.data.forEach((item, index) => {
                        rawItem += `<li><a class="timing ${index === 0 ? 'active_select' : ''} ${item.Disabled ? 'disable' : ''}"  href="#" data-value="${item.Value}">${item.Text}</a></li>`;
                    });
                    $el.html(rawItem);
                },
                (jqXhr, textStatus) => {

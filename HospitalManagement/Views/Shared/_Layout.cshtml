                    });
                }
            )
        }
        function loadFrameTimes(doctorId, _successCallBack, _errorCallBack) {
            var formData = new FormData();
            formData.append('doctorId', doctorId);
            formData.append('time', $('.date-time-select').val());
            ajax.createOrUpdate('@Url.Action("GetListFrameTimes", "Doctor")', formData, _successCallBack, _errorCallBack);
        }
        function setHtml(obj) {
            $("#ImgAvt").attr("src", obj.Avatar);

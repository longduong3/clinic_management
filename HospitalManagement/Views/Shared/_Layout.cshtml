<script type="text/javascript">
        var scrollButton = $(".Scroll-up");
        $(window).on("scroll", function () {
            if ($(this).scrollTop() >= 500) {
                scrollButton.show();
            } else {
                scrollButton.hide();
            }
        });
        scrollButton.on("click",
            function () {
                $("html,body").animate({
                    scrollTop: 0
                },
                    1e3);
            });

        function getDataDoctor(doctorId) {

            ajax.get(`/doctor/getJson`,
                { id: doctorId },
                (response) => {
                    if (response.status) {
                        const obj = response.data;
                        if (obj) {
                            //$("#ImgAvt").attr("src", obj.Avatar);
                            //$(".link-to-detail-doctor").attr("href", `/doctor/detail?id=${obj.Id}`);
                            //$("#doctor-name").text(obj.Name);
                            //$("#doctor-id").val(obj.Id);
                            //$("#doctor-address").text(obj.Address);

                            //$("#exampleModal").modal('show');
                            setHtml({});
                            setHtml(obj);
                        }
                    }
                },
                (jqXhr, textStatus) => {
                    const mess = `Request failed: ${textStatus}`;
                    Lobibox.alert("error", {
                        msg: "Đăng nhập để tiếp tục!"
                    });
                }
            );
        }
        function bookNow(doctorId) {
            getDataDoctor(doctorId);
            initFrameTimes(doctorId);
        }
        function dateTimeSelectOnChange() {
            var doctorId = $("#doctor-id").val();
            initFrameTimes(doctorId);
        }
        function saveBooking() {
            debugger;
            var doctorId = $("#doctor-id").val();
            var formData = new FormData(document.getElementById('form-book-doctor'));
            const selectTimeValue = $('.time-slot ul li a.timing.active_select').data('value');
            if (!selectTimeValue) {
                Lobibox.alert("error", {
                    msg: 'Vui lòng chọn khung giờ!'
                });
                return;
            }
            formData.append("frameTime", selectTimeValue);

            if ($('#momoRadio').is(':checked')) {
                ajax.createOrUpdate(`/home/GetPaymentMomoUrl`,
                    formData,
                    (response) => {
                        if (response.status) {
                            window.location.href = response.url;
                        } else {
                            if (response.isExist) {
                                //Lobibox.alert("error", {
                                //    msg:
                                //});
                                Lobibox.confirm({
                                    msg: response.mess,
                                    callback: function ($this, type, ev) {
                                        if (type === 'yes') {
                                            loadDoctorPropose(formData,
                                                (res) => {
                                                    if (res.status) {
                                                        if (res.isLoadInfo) {
                                                            getDataDoctor(res.data);
                                                            initFrameTimes(res.data);
                                                        } else {
                                                            Lobibox.alert("error", {
                                                                msg: res.mess
                                                            });
                                                        }
                                                    }
                                                },
                                                (jqXhr, textStatus) => {
                                                    const mess = `Request failed: ${textStatus}`;
                                                    Lobibox.alert("error", {
                                                        msg: mess
                                                    });
                                                }
                                            );
                                        }
                                    }
                                });
                                return;
                            }
                            Lobibox.alert("error", {
                                msg: response.mess
                            });
                        }
                    },
                    (jqXhr, textStatus) => {
                        const mess = `Request failed: ${textStatus}`;
                        Lobibox.alert("error", {
                            msg: mess
                        });
                    }
                )
            }
            ////
            else if ($('#vnpayRadio').is(':checked')) {
                ajax.createOrUpdate(`/home/GetPaymentMomoUrl`,
                    formData,
                    (response) => {
                        if (response.status) {
                        window.location.href = '@Url.Action("Payment")';
                        initFrameTimes(doctorId);
                        } else {
                            if (response.isExist) {
                                Lobibox.confirm({
                                    msg: response.mess,
                                    callback: function ($this, type, ev) {
                                        if (type === 'yes') {
                                            loadDoctorPropose(formData,
                                                (res) => {
                                                    if (res.status) {
                                                        if (res.isLoadInfo) {
                                                            getDataDoctor(res.data);
                                                            initFrameTimes(res.data);
                                                        } else {
                                                            Lobibox.alert("error", {
                                                                msg: res.mess
                                                            });
                                                        }
                                                    }
                                                },
                                                (jqXhr, textStatus) => {
                                                    const mess = `Request failed: ${textStatus}`;
                                                    Lobibox.alert("error", {
                                                        msg: mess
                                                    });
                                                }
                                            );
                                        }
                                    }
                                });
                                return;
                            }
                            Lobibox.alert("error", {
                                msg: response.mess
                            });
                        }
                    },
                    (jqXhr, textStatus) => {
                        const mess = `Request failed: ${textStatus}`;
                        Lobibox.alert("error", {
                            msg: mess
                        });
                    }
                )
            }
            else if ($('#firstbooking').is(':checked')) {
                ajax.createOrUpdate(`/doctor/book`,
                    formData,
                    (response) => {
                        if (response.status) {
                            Lobibox.alert("success", {
                            msg: response.mess
                      });
                      initFrameTimes(doctorId);
                        } else {
